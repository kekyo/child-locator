    "@testing-library/react": ">=16.0.0",
    "@testing-library/user-event": ">=14.0.0",
    "@types/node": ">=22.0.0",
    "@types/react": ">=19.0.0",
    "@types/react-dom": ">=19.0.0",
    "@vitejs/plugin-react": ">=5.0.0",
    "globals": ">=16.0.0",
    "jsdom": ">=26.0.0",
    "prettier-max": ">=1.9.0",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0",
    "screw-up": ">=1.10.0",
    "typescript": ">=5.0.0",
